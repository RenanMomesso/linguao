{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@/*": ["*"],
      "@/components/*": ["components/*"],
      "@/theme/*": ["theme/*"],
      "@/utils/*": ["utils/*"],
      "@/hooks/*": ["hooks/*"],
      "@/services/*": ["services/*"],
      "@/store/*": ["store/*"],
      "@/images/*": ["images/*"],
      "@/navigation/*": ["navigation/*"],
      "@/screens/*": ["screens/*"],
      "@/types/*": ["types/*"],
      "@/assets/*": ["assets/*"],
      "@/graphql/*": ["graphql/*"],
      
    }
  }
}

